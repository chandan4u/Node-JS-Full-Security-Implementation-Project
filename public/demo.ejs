<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <title>Empower flexi</title>
    <style>
        /* cyrillic-ext */
        @font-face {
            font-family: 'Roboto';
            font-style: normal;
            font-weight: 300;
            src: local('Roboto Light'), local('Roboto-Light'), url(https://fonts.gstatic.com/s/roboto/v16/0eC6fl06luXEYWpBSJvXCIX0hVgzZQUfRDuZrPvH3D8.woff2) format('woff2');
            unicode-range: U+0460-052F, U+20B4, U+2DE0-2DFF, U+A640-A69F;
        }

        /* cyrillic */
        @font-face {
            font-family: 'Roboto';
            font-style: normal;
            font-weight: 300;
            src: local('Roboto Light'), local('Roboto-Light'), url(https://fonts.gstatic.com/s/roboto/v16/Fl4y0QdOxyyTHEGMXX8kcYX0hVgzZQUfRDuZrPvH3D8.woff2) format('woff2');
            unicode-range: U+0400-045F, U+0490-0491, U+04B0-04B1, U+2116;
        }

        /* greek-ext */
        @font-face {
            font-family: 'Roboto';
            font-style: normal;
            font-weight: 300;
            src: local('Roboto Light'), local('Roboto-Light'), url(https://fonts.gstatic.com/s/roboto/v16/-L14Jk06m6pUHB-5mXQQnYX0hVgzZQUfRDuZrPvH3D8.woff2) format('woff2');
            unicode-range: U+1F00-1FFF;
        }

        /* greek */
        @font-face {
            font-family: 'Roboto';
            font-style: normal;
            font-weight: 300;
            src: local('Roboto Light'), local('Roboto-Light'), url(https://fonts.gstatic.com/s/roboto/v16/I3S1wsgSg9YCurV6PUkTOYX0hVgzZQUfRDuZrPvH3D8.woff2) format('woff2');
            unicode-range: U+0370-03FF;
        }

        /* vietnamese */
        @font-face {
            font-family: 'Roboto';
            font-style: normal;
            font-weight: 300;
            src: local('Roboto Light'), local('Roboto-Light'), url(https://fonts.gstatic.com/s/roboto/v16/NYDWBdD4gIq26G5XYbHsFIX0hVgzZQUfRDuZrPvH3D8.woff2) format('woff2');
            unicode-range: U+0102-0103, U+1EA0-1EF9, U+20AB;
        }

        /* latin-ext */
        @font-face {
            font-family: 'Roboto';
            font-style: normal;
            font-weight: 300;
            src: local('Roboto Light'), local('Roboto-Light'), url(https://fonts.gstatic.com/s/roboto/v16/Pru33qjShpZSmG3z6VYwnYX0hVgzZQUfRDuZrPvH3D8.woff2) format('woff2');
            unicode-range: U+0100-024F, U+1E00-1EFF, U+20A0-20AB, U+20AD-20CF, U+2C60-2C7F, U+A720-A7FF;
        }

        /* latin */
        @font-face {
            font-family: 'Roboto';
            font-style: normal;
            font-weight: 300;
            src: local('Roboto Light'), local('Roboto-Light'), url(https://fonts.gstatic.com/s/roboto/v16/Hgo13k-tfSpn0qi1SFdUfZBw1xU1rKptJj_0jans920.woff2) format('woff2');
            unicode-range: U+0000-00FF, U+0131, U+0152-0153, U+02C6, U+02DA, U+02DC, U+2000-206F, U+2074, U+20AC, U+2212, U+2215;
        }

        /* latin-ext */
        @font-face {
            font-family: 'Raleway';
            font-style: normal;
            font-weight: 400;
            src: local('Raleway'), local('Raleway-Regular'), url(https://fonts.gstatic.com/s/raleway/v11/YZaO6llzOP57DpTBv2GnyFKPGs1ZzpMvnHX-7fPOuAc.woff2) format('woff2');
            unicode-range: U+0100-024F, U+1E00-1EFF, U+20A0-20AB, U+20AD-20CF, U+2C60-2C7F, U+A720-A7FF;
        }

        /* latin */
        @font-face {
            font-family: 'Raleway';
            font-style: normal;
            font-weight: 400;
            src: local('Raleway'), local('Raleway-Regular'), url(https://fonts.gstatic.com/s/raleway/v11/QAUlVt1jXOgQavlW5wEfxQLUuEpTyoUstqEm5AMlJo4.woff2) format('woff2');
            unicode-range: U+0000-00FF, U+0131, U+0152-0153, U+02C6, U+02DA, U+02DC, U+2000-206F, U+2074, U+20AC, U+2212, U+2215;
        }

        html, body, h1, h2, h3, h4, h5, h6, p, blockquote {
            margin: 0;
            padding: 0;
            border: 0;
        }

        body {
            color: #313A4D;
            font-family: "Raleway", sans-serif;
        }

        table {
            border-collapse: collapse;
            border-spacing: 0;
        }

        tr.tex-px {
            height: 10px;
        }

        @media (max-width: 769px) {
            table {
                width: 96% !important;
                margin: 2% !important;
            }
        }

    </style>

</head>

<body>


<table width="100%" border="0" cellspacing="0" cellpadding="0" align="center">
    <tbody>
    <tr>
        <td colspan="3" align="right"><img src="http://localhost:8282/images/tillogo_new.png"
                                           alt="Times Internet Limited" width="150"
                                           style="padding:10px 0px;"></td>
    </tr>
    <tr>
        <td colspan="3">&nbsp;</td>
    </tr>
    <tr>
        <td colspan="3" align="center" style=" font-size:28px; text-transform:uppercase;">Salary Structure</td>
    </tr>
    <tr>
        <td colspan="3" align="center" style=" font-size:18px; text-transform:normal; color:#0095da;">flexitool 2017
        </td>
    </tr>
    <tr>
        <td colspan="3">&nbsp;</td>
    </tr>

    <tr>
        <td colspan="3" style="font-size:17px" height="20"><strong><%= data.Name %></strong></td>
    </tr>
    <tr>
        <td colspan="3" style="font-size:17px;" height="20"><%= data.Business %></td>
    </tr>
    <tr>
        <td colspan="3" style="font-size:17px;" height="20"><%= data.Sap_Id %></td>
    </tr>
    <tr>
        <td colspan="3" style="font-size:17px;" height="20"><%= moment(data.Updated_at).format('DD MMM, YYYY') %></td>
    </tr>
    <tr>
        <td colspan="3">&nbsp;</td>
    </tr>

    <tr>
        <td colspan="3" style="font-size:16px;" height="40">Hi <%= data.Name %>,</td>
    </tr>

    <tr>
        <td colspan="3" style="font-size:16px;">Please find below your current and revised salary structure.The changes
            come in effect from 1 August 2017.
        </td>
    </tr>

    <tr>
        <td colspan="3">&nbsp;</td>
    </tr>

    <tr>
        <td style="font-size:12px; font-weight:bold;text-transform:uppercase;color:#0095da;"
            width="50%" height="35">Payslip Items
        </td>
        <td style="font-size:12px; text-align: center; font-weight:bold;text-transform:uppercase;color:#0095da;"
            width="25%" height="35">Current INR<br/>Annual
        </td>
        <td style="font-size:12px; text-align: center; font-weight:bold;text-transform:uppercase;color:#0095da;"
            width="25%" height="35">Revised INR<br/>Annual
        </td>

    </tr>


    <tr>
        <td style="font-size:16px; font-weight:normal;" width="50%"
            height="25">Basic Pay
        </td>
        <td style="font-size:16px; font-weight:normal; text-align: center;" width="25%"
            height="25"><%= data.Basic %>
        </td>
        <td style="font-size:16px; font-weight:normal; text-align: center;" width="25%"
            height="25"><%= data.Basic_Total %>
        </td>

    </tr>
    <tr>
        <td style="font-size:16px; font-weight:normal;" width="50%" height="25">House rent Allowance
        </td>
        <td style="font-size:16px; font-weight:normal; text-align: center;" width="25%"
            height="25"><%= data.Hra_Current %>
        </td>
        <td style="font-size:16px; font-weight:normal; text-align: center;" width="25%"
            height="25"><%= data.Hra_Total %>
        </td>

    </tr>
    <tr>
        <td style="font-size:16px; font-weight:normal;" width="50%"
            height="25">Special Allowance
        </td>
        <td style="font-size:16px; font-weight:normal; text-align: center;" width="25%"
            height="25"><%= data.Special_Allowance_Current %>
        </td>
        <td style="font-size:16px; font-weight:normal; text-align: center;" width="25%"
            height="25"><%= data.Special_Allowance_Total %>
        </td>

    </tr>

    <% if(data.Transport_Allowance_Current != '0' || data.Transport_Allowance_Total != '0'){ %>
    <tr>
        <td style="font-size:16px; font-weight:normal;" width="50%"
            height="25">Transport Allowance
        </td>
        <td style="font-size:16px; font-weight:normal; text-align: center;" width="25%"
            height="25"><%= data.Transport_Allowance_Current %>
        </td>
        <td style="font-size:16px; font-weight:normal; text-align: center;" width="25%"
            height="25"><%= data.Transport_Allowance_Total %>
        </td>
    </tr>
    <% } %>

    <% if(data.Conveyance_Reimbursement_Current != '0' || data.Conveyance_Reimbursement_Total != '0' || data.Communication_Reimbursement_Current != '0' || data.Communication_Reimbursement_Total != '0' || data.Medical_Reimbursement_Current != '0' || data.Medical_Reimbursement_Current != '0' || data.LTA_Current != '0' || data.LTA_Total != '0' || data.Meal_Voucher_Old_Status || data.Meal_Voucher_New_Status){ %>
    <tr>
        <td colspan="3" style="font-size:12px; font-weight:bold;text-transform:uppercase;color:#0095da;"
            width="50%" height="35">
            Rembursements
        </td>
    </tr>
    <% } %>

    <% if(data.Conveyance_Reimbursement_Current != '0' || data.Conveyance_Reimbursement_Total != '0' ){ %>
    <tr>
        <td style="font-size:16px; font-weight:normal;" width="50%"
            height="25">Conveyance
        </td>
        <td style="font-size:16px; font-weight:normal; text-align: center;" width="25%"
            height="25"><%= data.Conveyance_Reimbursement_Current %>
        </td>
        <td style="font-size:16px; font-weight:normal; text-align: center;" width="25%"
            height="25"><%= data.Conveyance_Reimbursement_Total %>
        </td>
    </tr>
    <% } %>

    <% if(data.Communication_Reimbursement_Current != '0' || data.Communication_Reimbursement_Total != '0'){ %>
    <tr>
        <td style="font-size:16px; font-weight:normal;" width="50%"
            height="25">Communication
        </td>
        <td style="font-size:16px; font-weight:normal; text-align: center;" width="25%"
            height="25"><%= data.Communication_Reimbursement_Current %>
        </td>
        <td style="font-size:16px; font-weight:normal; text-align: center;" width="25%"
            height="25"><%= data.Communication_Reimbursement_Total %>
        </td>
    </tr>
    <% } %>

    <% if(data.Medical_Reimbursement_Current != '0' || data.Medical_Reimbursement_Current != '0'){ %>
    <tr>
        <td style="font-size:16px; font-weight:normal;" width="50%"
            height="25">Medical
        </td>
        <td style="font-size:16px; font-weight:normal; text-align: center;" width="25%"
            height="25"><%= data.Medical_Reimbursement_Current %>
        </td>
        <td style="font-size:16px; font-weight:normal; text-align: center;" width="25%"
            height="25"><%= data.Medical_Reimbursement_Total %>
        </td>
    </tr>
    <% } %>

    <% if(data.LTA_Current != '0' || data.LTA_Total != '0'){ %>
    <tr>
        <td style="font-size:16px; font-weight:normal;" width="50%"
            height="25">Leave Travel Assistance
        </td>
        <td style="font-size:16px; font-weight:normal; text-align: center;" width="25%"
            height="25"><%= data.LTA_Current %>
        </td>
        <td style="font-size:16px; font-weight:normal; text-align: center;" width="25%"
            height="25"><%= data.LTA_Total %>
        </td>
    </tr>
    <% } %>

    <% if(data.Meal_Voucher_Old_Status == "true" || data.Meal_Voucher_New_Status == "true"){ %>
    <tr>
        <td style="font-size:16px; font-weight:normal;" width="50%"
            height="25">Meal Voucher 
        </td>

        <td style="font-size:16px; font-weight:normal; text-align: center;" width="25%"
            height="25">
            <% if(data.Meal_Voucher_Old_Status == "true"){ %> <%= mealVoucherAmount %> <%}else{%><%= zeroPrintConstant %><%}%>
        </td>

        <td style="font-size:16px; font-weight:normal; text-align: center;" width="25%"
            height="25"><%= data.Meal_Voucher_Total %>
        </td>
    </tr>
    <% } %>

    <%if(data.BPL_Current > 0){ %>
    <tr>
        <td style="font-size:16px; font-weight:normal;" width="50%"
            height="25">Business Promotion 
        </td>

        <td style="font-size:16px; font-weight:normal; text-align: center;" width="25%"
            height="25">
            <%= data.BPL_Current %>
        </td>

        <td style="font-size:16px; font-weight:normal; text-align: center;" width="25%"
            height="25"><%= data.BPL_Total %>
        </td>
    </tr>
    <% } %>
    <%if(data.CarMonetization_Current > 0) { %>
    <tr>
        <td style="font-size:16px; font-weight:normal;" width="50%"
            height="25">Car Monetization 
        </td>

        <td style="font-size:16px; font-weight:normal; text-align: center;" width="25%"
            height="25">
            <%= data.CarMonetization_Current %>
        </td>

        <td style="font-size:16px; font-weight:normal; text-align: center;" width="25%"
            height="25"><%= data.CarMonetization_Total %>
        </td>
    </tr>
    <% } %>

    <tr>
        <td colspan="3" style="font-size:12px; font-weight:bold;text-transform:uppercase;color:#0095da;"
            width="50%" height="35">Retirals
        </td>
    </tr>


    <tr>
        <td style="font-size:16px; font-weight:normal;"
            width="50%" height="25">PF&nbsp;<span
                    style="font-size:14px; font-weight:normal;">Employer Contribution</span>
        </td>
        <td style="font-size:16px; font-weight:normal; text-align: center;" width="25%"
            height="25"><%= data.PF_Current %>
        </td>
        <td style="font-size:16px; font-weight:normal; text-align: center;" width="25%" height="25"><%= data.PF_Total %>
        </td>

    </tr>

    <tr>
        <td style="font-size:16px; font-weight:normal;"
            width="50%" height="25">Gratuity
        </td>
        <td style="font-size:16px; font-weight:normal; text-align: center;" width="25%"
            height="25"><%= data.Gratuity_Current %>
        </td>
        <td style="font-size:16px; font-weight:normal; text-align: center;" width="25%"
            height="25"><%= data.Gratuity_Total %>
        </td>

    </tr>

    <%if(data.TVP_Total > 0) { %>

    <tr>
        <td colspan="3" style="font-size:12px; font-weight:bold;text-transform:uppercase;color:#0095da;"
            width="50%" height="35">Variable
        </td>
    </tr>
    <tr>
        <td style="font-size:16px; font-weight:normal;" width="50%" height="25">Variable Pay
        </td>
        <td style="font-size:16px; font-weight:normal; text-align: center;" width="25%"
            height="25"><%= data.TVP_Total %>
        </td>
        <td style="font-size:16px; font-weight:normal; text-align: center;" width="25%"
            height="25"><%= data.TVP_Total %>
        </td>
    </tr>

    <% } %>

    <tr>
        <td colspan="3">&nbsp;</td>
    </tr>

    <tr>
        <td style="font-size:14px; font-weight:bold;text-transform:uppercase; border-top:1px solid #313A4D; border-bottom:1px solid #313A4D;"
            width="50%" height="30">Total Target Remuneration
        </td>
        <td style="font-size:18px; font-weight:bold;text-transform:uppercase;text-align: center; border-top:1px solid #313A4D; border-bottom:1px solid #313A4D;"
            width="25%" height="30"><%= data.TTR_Current %>
        </td>
        <td style="font-size:18px; font-weight:bold;text-transform:uppercase;text-align: center; border-top:1px solid #313A4D; border-bottom:1px solid #313A4D;"
            width="25%" height="30"><%= data.TTR_Current %>
        </td>

    </tr>

    <tr>
        <td colspan="3">&nbsp;</td>
    </tr>

    <tr>
        <td colspan="3" style="font-size:12px;">This is a computer generated document:</td>
    </tr>

    <tr>
        <td colspan="3">&nbsp;</td>
    </tr>
    <tr>
        <td colspan="3">&nbsp;</td>
    </tr>
    <tr>
        <td colspan="3">&nbsp;</td>
    </tr>
    <tr>
        <td colspan="3" style="margin:0; text-align:center;" valign="top">
            <p style="margin:5px 0px; font-size:12px; color:#000000;">
                Corp. Office: Plot No 391, Udyog Vihar, Phase-III Gurgaon - 122016, Haryana, Tel:
                +91-124418550.<br>
                Registered Office: 10, Daryaganj, New Delhi - 110022. Tel: +91-11-3984333. Fax:
                +91-11-23279643, 23243081<br>
                CIN:U74999PLC135531

            </p>
        </td>
    </tr>

    </tbody>
</table>


</body>
</html>
